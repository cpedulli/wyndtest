{
 "manifest_version": 2,
 "name": "Wynd",
 "version": "1.0.0",
 "description": "This is a sample description",
 "short_name": "Short Sample Name",
 "permissions": ["activeTab", "declarativeContent", "storage", "bookmarks"],
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "css": ["background.css"],
     "js": ["background.js"]
   }
 ],
 "browser_action": {
   "default_title": "Does a thing when you do a thing",
   "default_popup": "popup.html",
   "default_icon": {
     "16": "https://github.com/cpedulli/wyndtest/blob/master/wynd%20pfp.png",
     "32": "https://github.com/cpedulli/wyndtest/blob/master/wynd%20pfp.png"
   }
 }
}
